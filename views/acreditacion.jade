extends menu
block title
  title!=title
block scriptpage
  script(src='/javascripts/acreditacion_alta_calidad.js')
block resto
  .col-md-12(style='text-align:left')
      input#btnfil.btn.btn-warning.btn-small(type='button' value='Filtros' onCLick='opendivfilter()')
      br
  #divfilter.modal.fade.bd-example-modal-lg
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#modal-title.modal-title Filtros
        .modal-body
          form#frmfilter(method='POST' action='consultaFiltradaAcreditacion')
            br
            .col-md-6
              .col-sm-6
                .col-md-3  
                  label(for='lstanho1') Años De:
                .col-md-9
                  select#lstanho1.form-control
              .col-sm-6
                .col-md-3
                  label(for='lstanho2') a
                .col-md-9
                  select#lstanho2.form-control
            .col-md-1  
              input(type='submit' value='Filtrar' class='btn btn-info' )
        .modal-footer
  #divupdate.modal.fade.bd-example-modal-lg
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#modal-title.modal-title Estado de Acreditación
        .modal-body
          form#frmupdate(method='POST' action='actualizaAltaCalidad')
            br
            table.table.table-hover.table-bordered.table-responsed
              tbody.col-sm-3
              tbody.col-sm-6
                tr
                  label(for='programa')
                  td Codigo del programa:
                  td#cod
                tr
                  label(for='fechaini') 
                  td Fecha de Inicio de Acreditación:
                  td#ini
                tr  
                  label(for='periodo') 
                  td Periodo:
                  td#per
                tr  
                  td(colspan="2")
                    input(type='submit' value='Acreditar' class='btn btn-info' )
              tbody.col-sm-3
        .modal-footer
  .col-md-12
    h3 Panorama General Programas Acreditados en Alta Calidad
    .col-md-6
      #divgraph1.col-md-12
      .col-md-12
        select#graph1
          option(selected='', value='1') Seleccione un tipo de gráfica
          optgroup(label='Columnas')
            option(value='1') 2D
            option(value='2') 3D
          optgroup(label='Barras')
            option(value='5') 2D
            option(value='6') 3D
          optgroup(label='Tendencia')
            option(value='3') Línea
            option(value='4') Area
    #divtable.col-md-6
      h6#goal
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Año
            th.th No Acreditados Pregrado
            th.th Total Programas Pregrado
            th.th Indicador(%)
            th.th Estado
        tbody#tableres
    .col-md-6
        br
        br
    h3#lblper
    .col-md-6
      #divgraph3.col-md-12
      .col-md-12
        select#graph2
          option(selected='', value='1') Seleccione un tipo de gráfica
          optgroup(label='Columnas')
            option(value='1') 2D
            option(value='2') 3D
          optgroup(label='Barras')
            option(value='4') 2D
            option(value='5') 3D
          optgroup(label='Tendencia')
            option(value='3') Línea
    #divgraph2.col-md-6
    .col-md-6
        br
        br
    #divtable2.col-md-6(style!=rols)
      #anhoactual
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Programa Académico
            th.th Estado
        tbody#tableresprogram
    #divtable3.col-md-6(style!=rols)
      #anhoactual2
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Programa Académico
        tbody#tableresprogram2
  #myModal.modal.fade
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
            h4#modal-title.modal-title Error De Filtros
          #modal-body.modal-body
            p No seleccione el mismo año dos veces 
          .modal-footer
            button.btn.btn-primary(type='button', onclick='hidenmodal()') Aceptar
  //campo oculto para manterne le json del contenido
  input(id='txtjson' type="hidden")
  // campo oculto para controlar rols
  //modal para los informes 
  #modalinfo.modal.fade.bd-example-modal-lg
    #modalinfobody.modal-content
      #modal-bodyinfo.modal-body.table-responsive
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
        input.btn.btn-info(type ='button' value='Imprimir' onclick='pdf()')
        br
        table#tabinfo.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
          tr
            td
              img.drap(src='/images/escudo.png')
            td
              center
                h3 SISTEMA DE GESTIÓN DE CALIDAD
                h3
                  b Manual de Indicador- 
            td
              table.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
                tr
                  td Código: SGC-FR-04
                tr
                  td Página: 1 de 2
                tr
                  td Versión: 3
                tr
                  td Vigente a partir de: 21/06/2010
        br
        table#tabind.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
          tr
            td(colspan='2')
              h5(style='text-align:center') DATOS DEL INDICADOR
          tr
            td 1. PROCESO:
            td
              ul
                li
                  | Capacidad Directiva
                li
                  | Proyección Social
                li
                  | Universidad y Región
          tr
            td 2. LÍDER:
            td Vicerrector(a) Académico(a)
          tr
            td 3. OBJETIVO DEL PROCESO:
            td
              p
                | Establecer el porcentaje de programas 
                | académicos de pregrado acreditados en 
                | la Universidad de Nariño
          tr
            td 4. NOMBRE DEL INDICADOR:
            td
              | Programas Académicos de 
              | Pregrado con Acreditación de Alta Calidad.
          tr
            td 5. ATRIBUTO A MEDIR:
            td
              | Porcentaje de programas Académicos de 
              | Pregrado con Acreditación de Alta Calidad.
          tr
            td 6. OBJETIVO DE CALIDAD AL CUAL CONTRIBUYE:
            td
              ul
                li
                  | C D: Implementar una administración eficaz, eficiente, efectiva y transparente,
                  | a través de la descentralización de la función académica, la 
                  | administrativa y financiera, para el mejoramiento de todos los procesos con el 
                  | fin de cumplir la misión de docencia, investigación y proyección 
                  | social.
                li
                  | U Y R: Establecer la convivencia responsable de la Universidad con la región, 
                  | entendida como una cosmovisión compleja que se comprende, se reconoce y se 
                  | autorregula, que está en permanente cambio e interactúa con otros ámbitos de la realidad, 
                  | con seres humanos solidarios capaces de entender y respetar al otro 
                  | como igual y diferente y que genera condiciones de equidad y libertad.
          tr
            td 7. TIPO DE INDICADOR:
            td ESTRATÉGICO
          tr
            td 8. FRECUENCIA:
            td Anual
          tr
            td 9. PERIODO DE CALCULO:
            td Último mes antes de finalizar el año
          tr
            td 10. TENDENCIA ESPERADA:
            td Incremento
          tr
            td 11. META
            td
              | Acreditar  el 20% de los Programas Académicos 
              | ofrecidos por la Universidad.
          tr
            td 12. OBJETIVO DEL INDICADOR:
            td
              |Establecer el porcentaje de programas académicos de pregrado acreditados en la Universidad de Nariño
          tr
            td 13. RANGO Y FORMA DE INTERPRETACIÓN:
            td
              input.form-control(id='liminf' type='text', value='10%', width='5') 
          tr
            td 14. FÓRMULA:
            td
              b Por cohorte:
              br
              | (Número de programas Acreditados en Alta Calidad/Total de 
              | programas de Pregrado de  la Universidad de Nariño)*100
          tr
            td 15. MANERA QUE PODÍA SER GRAFICADO:
            td
              | Diagrama de Bloque, señalando el
              | porcentaje obtenido de deserción, frente a la meta.
          tr
            td 16. PUNTO DE REGISTRO:
            td  Estado de los Procesos de Acreditación en Alta Calidad según el Aplicativo   SACES.
          tr
            td 17. RESPONSABLE DEL CALCULO
            td
              | Vicerrectoría Académica 
              | Asesor de Desarrollo Académico de la Vicerrectoría Académica
          tr
            td 18. INSTRUCTIVO:
            td
              | Al finalizar cada año se aplica la fórmula y se confronta los resultados
              | del año anterior con los del 
              | año actual.
        br
        table#tabindres.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
          tr
            td(colspan='2')
              h5(style='text-align:center') RESULTADOS Y EVOLUCIÓN DEL INDICADOR
          tr
            td 19. FECHA DE CALCULO:
            td 22 de  noviembre  de 2015
          tr
            td 20. RESULTADO DEL INDICADOR:
            td 
              #divtab(style='width: 100%; height: 40%;')
          tr
            td 21. CALIFICACIÓN:
            td
              textarea#txtcal.form-control(cols='100', rows='3')
          tr
            td 22. LECTURA DE LA EVOLUCIÓN DEL INDICADOR:
            td
              textarea#txtlec.form-control(cols='100', rows='6')
          tr
            td 23. EVOLUCIÓN GRÁFICA:
            //td aqui se colocan las las graficas 
            td 
              #divg1
              #divg2
              #divg3
