extends menu
block title
  title!=title
block scriptpage
  script(src='/javascripts/acreditacion_alta_calidad.js')
block resto
  .col-md-12(style='text-align:left')
      input#btnfil.btn.btn-warning.btn-small(type='button' value='Filtros' onCLick='opendivfilter()')
      br
  #divfilter.modal.fade.bd-example-modal-lg
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#modal-title.modal-title Filtros
        .modal-body
          form#frmfilter(method='POST' action='consultaFiltradaAcreditacion')
            br
            .col-md-6
              .col-sm-6
                .col-md-3  
                  label(for='lstanho1') Años De:
                .col-md-9
                  select#lstanho1.form-control
              .col-sm-6
                .col-md-3
                  label(for='lstanho2') a
                .col-md-9
                  select#lstanho2.form-control
            .col-md-1  
              input(type='submit' value='Filtrar' class='btn btn-info' )
        .modal-footer
  #divupdate.modal.fade.bd-example-modal-lg
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#modal-title.modal-title Estado de Acreditación
        .modal-body
          form#frmupdate(method='POST' action='actualizaAltaCalidad')
            br
            table.table.table-hover.table-bordered.table-responsed
              tbody.col-sm-3
              tbody.col-sm-6
                #flag
                tr
                  label(for='programa')
                  td Codigo del programa:
                  td#cod
                tr
                  label(for='fechaini') 
                  td Fecha de Inicio de Acreditación:
                  td#ini
                tr  
                  label(for='periodo') 
                  td Periodo:
                  td#per
                tr  
                  td(colspan="2")
                    input(type='submit' value='Acreditar' class='btn btn-info' )
              tbody.col-sm-3
        .modal-footer
  .col-md-12
    h3 Panorama General Programas Acreditados en Alta Calidad
    .col-md-6
      #divgraph1.col-md-12
      .col-md-12
        select#graph1
          option(selected='', value='1') Seleccione un tipo de gráfica
          optgroup(label='Columnas')
            option(value='1') 2D
            option(value='2') 3D
          optgroup(label='Barras')
            option(value='5') 2D
            option(value='6') 3D
          optgroup(label='Tendencia')
            option(value='3') Línea
            option(value='4') Area
    #divtable.col-md-6
      h6#goal
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Año
            th.th No Acreditados Pregrado
            th.th Total Programas Pregrado
            th.th Indicador(%)
            th.th.est Estado
        tbody#tableres
    .col-md-6
        br
        br
    h3#lblper
    .col-md-6
      #divgraph2.col-md-12
      .col-md-12
        select#graph2
          option(selected='', value='1') Seleccione un tipo de gráfica
          optgroup(label='Columnas')
            option(value='1') 2D
            option(value='2') 3D
          optgroup(label='Barras')
            option(value='4') 2D
            option(value='5') 3D
          optgroup(label='Tendencia')
            option(value='3') Línea
    #divgraph3.col-md-6
    .col-md-6
        br
        br
    #divtable2.col-md-6(style!=rols)
      #anhoactual
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Programa Académico
            th.th Estado
        tbody#tableresprogram
    #divtable3.col-md-6(style!=rols)
      #anhoactual2
      table.table.table-hover.table-bordered.table-responsed(border='1')
        thead 
          tr
            th.th Programa Académico
        tbody#tableresprogram2
  #myModal.modal.fade
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
            h4#modal-title.modal-title Error De Filtros
          #modal-body.modal-body
            p No seleccione el mismo año dos veces 
          .modal-footer
            button.btn.btn-primary(type='button', onclick='hidenmodal()') Aceptar
  //campo oculto para manterne le json del contenido
  input(id='txtjson' type="hidden")

  //modal para los informes 
  #modalinfo.modal.fade.bd-example-modal-lg
    #modalinfobody.modal-content
      #modal-bodyinfo.modal-body.table-responsive
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
        input.btn.btn-info(type ='button' value='PDF' onclick='getPDF()')
        input.btn.btn-warning(type ='button' value='Editar' style!=rols onclick='editRepor()')
        br
        table#tabind.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
          tr
            td(colspan='2')
              h5(style='text-align:center') DATOS DEL INDICADOR
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 1. PROCESO:
            td
              input#atrinfo1.form-control.atrinfo(type='text' value='Formación Académica' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 2. LÍDER:
            td 
              input#atrinfo2.form-control.atrinfo(type='text' value='Vicerrector(a) Académico(a)' readonly) 
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 3. OBJETIVO DEL PROCESO:
            td
              textarea#atrinfo3.form-control.atrinfo(cols='40', rows='2' readonly)
                | Formar integralmente estudiantes a través de los diferentes Programas, niveles y modalidades de Educación Superior.
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 4. NOMBRE DEL INDICADOR:
            td
              input#atrinfo4.form-control.atrinfo(type='text' value='Programas Acreditados en Alta Calidad' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 5. ATRIBUTO A MEDIR:
            td
              input#atrinfo5.form-control.atrinfo(type='text' value='Formación académica' readonly) 
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 6. OBJETIVO DE CALIDAD AL CUAL CONTRIBUYE:
            td
              textarea#atrinfo6.form-control.atrinfo(cols='40', rows='2' readonly)
                | Mejorar permanentemente la calidad en la docencia, investigación y proyección social de la Universidad.
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 7. TIPO DE INDICADOR:
            td 
              input#atrinfo7.form-control.atrinfo(type='text' value='Eficacia' readonly)  
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 8. FRECUENCIA:
            td 
              input#atrinfo8.form-control.atrinfo(type='text' value='Anual' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 9. PERIODO DE CALCULO:
            td
              input#atrinfo9.form-control.atrinfo(type='text' value='Último mes antes de finalizar el año' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 10. TENDENCIA ESPERADA:
            td
              input#atrinfo10.form-control.atrinfo(type='text' value='Incrementar' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 11. META
            td
              input#atrinfo11.form-control.atrinfo(type='text' value='Lograr el 15% de los programas acreditables en Alta Calidad' readonly)  
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 12. OBJETIVO DEL INDICADOR:
            td
              textarea#atrinfo12.form-control.atrinfo(cols='40', rows='2' readonly)
                | Medir el porcentaje de los programas académicos acreditados en alta calidad
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 13. RANGO Y FORMA DE INTERPRETACIÓN:
            td
              input#atrinfo13.form-control.atrinfo(type='text', value='10%')
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 14. FÓRMULA:
            td
              textarea#atrinfo14.form-control.atrinfo(cols='40', rows='2' readonly)
                | (Número de programas Acreditados en Alta Calidad/Total de 
                | programas de Pregrado de  la Universidad de Nariño)*100
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 15. MANERA QUE PODÍA SER GRAFICADO:
            td
              input#atrinfo15.form-control.atrinfo(type='text' value='Diagrama de Bloque, señalando la relación de los programas acreditados' readonly)
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 16. PUNTO DE REGISTRO:
            td
              input#atrinfo16.form-control.atrinfo(type='text' value='Sistema de Información Académico' readonly)  
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 17. RESPONSABLE DEL CALCULO
            td
              input#atrinfo17.form-control.atrinfo(type='text' value='Profesional Sistema de Autoevaluación, Acreditación y Certificación' readonly)  
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 18. INSTRUCTIVO:
            td
              input#atrinfo18.form-control.atrinfo(type='text', value='Con la información suministrada por la División de Autoevaluación, Acreditación y Certificación, se aplica la fórmula establecida en el manual del indicador y se analiza los resultados' readonly)
        br
        table#tabindres.table.table-condensed.table-striped.table-hover.table-bordered.table-responsed(border='1')
          tr
            td(colspan='2')
              h5(style='text-align:center') RESULTADOS Y EVOLUCIÓN DEL INDICADOR
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 19. FECHA DE CALCULO:
            td 
              label#fecha
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 20. RESULTADO DEL INDICADOR:
            td 
              #divtab(style='width: 100%; height: 40%;')
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 21. CALIFICACIÓN:
            td
              textarea#txtcal.form-control(cols='40', rows='3')
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 22. LECTURA DE LA EVOLUCIÓN DEL INDICADOR:
            td
              textarea#txtlec.form-control(cols='40', rows='6')
          tr
            td(style='font-size: 14px;width: 15%;text-align: left;') 23. EVOLUCIÓN GRÁFICA:
            td 
              #divg1
              #divg2
